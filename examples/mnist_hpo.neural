network MNISTClassifier {
  input: (28, 28, 1)
  layers:
    Flatten()
    Dense(HPO(choice(128, 256)))
    Dropout(HPO(range(0.3, 0.7, step=0.1)))
    Output(10, "softmax")
  optimizer: Adam(learning_rate=HPO(log_range(1e-4, 1e-2)))
  train {
    epochs: 10
    batch_size: HPO(choice(16, 32, 64))
    search_method: "bayesian"
  }
}
