network TextClassifier {
    input: (None, 100)
    layers:
        Embedding(input_dim=10000, output_dim=128)
        LSTM(units=64, return_sequences=true)
        LSTM(units=64)
        Dense(units=64, activation="relu")
        Dropout(rate=0.5)
        Dense(units=1, activation="sigmoid")
    loss: "binary_crossentropy"
    optimizer: "Adam"
}
